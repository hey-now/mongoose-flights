<%- include('../partials/header') %>

<div id="newDiv">Before adding, please ensure that the Ticket is not in the following list:</div>
<br><br><section id="badge-container">
  <% tickets.forEach(function(t) { %>
    <article class="badge"><%= t.seat %></article>
  <% }) %>
</section><br><br>

<form id="add-ticket-form" action="/tickets" method="POST">
  <label>Seat:</label>
  <input type="text" name="seat" required pattern="[A-F][1-9]\d?">
  <label>Price:</label>
  <span>$</span><input type="number" name="price">
  <input type="submit" value="Add Ticket">
</form>

<%- include('../partials/footer') %></input>